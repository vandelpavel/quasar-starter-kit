    "vue-property-decorator": "^8.3.0",
    {{/if_eq}}
    {{#preset.axios}}"axios": "^0.18.1",{{/preset.axios}}
    {{#preset.i18n}}"vue-i18n": "^8.0.0",{{/preset.i18n}}
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "quasar": "^1.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-svg-inline-loader": "^1.5.0"

  },
  "devDependencies": {
    {{#preInstalledAppExtensions.animate}}"@dreamonkey/quasar-app-extension-animate": "^0.0.5",{{/preInstalledAppExtensions.animate}}
