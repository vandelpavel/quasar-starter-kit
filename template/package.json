{
  "name": "{{ name }}",
  "version": "0.0.1",
  "description": "{{ description }}",
  "productName": "{{ productName }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    {{#preset.lint}}"lint": "eslint --ext .js,{{#preset.typescript}}.ts,{{/preset.typescript}}.vue ./",{{/preset.lint}}
